{
  "name": "my-nextjs-starter",
  "version": "1.0.0",
  "main": "index.js",
  "packageManager": "pnpm@8.14.3",
  "scripts": {
    "start-by-docker": "echo $COMMIT_HASH && npx prisma migrate deploy && node server.js",
    "start": "npx prisma migrate deploy && next start",
    "dev": "PORT=3033 next dev",
    "build": "next build",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext ts,tsx . --ignore-path .gitignore",
    "format": "prettier --write './**/*.{js,ts,tsx,css,scss,json,html,graphql,md}' --ignore-path .gitignore",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "gen-c": "scaffdog generate react-component",
    "migrate": "dotenv -e .env.development -- prisma migrate dev",
    "migrate:reset": "dotenv -e .env.development -- prisma migrate reset",
    "migrate:reset:test": "dotenv -e .env.test -- prisma migrate reset --force --skip-seed",
    "migrate:deploy": "dotenv -e .env.production prisma migrate deploy",
    "prisma:studio": "dotenv -e .env.development -- prisma studio",
    "test": "dotenv -e .env -e .env.test -- prisma migrate reset --force --skip-seed && dotenv -e .env -e .env.test -- vitest",
    "test-ci": "dotenv -e .env.test -- prisma migrate reset --force --skip-seed && dotenv -e .env.test -- vitest --run --silent",
    "postinstall": "prisma generate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.4",
    "@prisma/client": "5.8.1",
    "@t3-oss/env-nextjs": "0.7.3",
    "@trpc/client": "10.45.0",
    "@trpc/next": "10.45.0",
    "@trpc/react-query": "10.45.0",
    "@trpc/server": "10.45.0",
    "@types/node": "20.11.6",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "10.4.17",
    "clsx": "2.1.0",
    "css-loader": "6.9.1",
    "dayjs": "1.11.10",
    "dotenv-cli": "7.3.0",
    "jotai": "2.6.2",
    "nanoid": "5.0.4",
    "next": "14.1.1",
    "pino": "8.17.2",
    "postcss": "8.4.33",
    "prisma": "5.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.49.3",
    "react-icons": "5.0.1",
    "remeda": "1.38.0",
    "sass": "1.70.0",
    "superjson": "2.2.1",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "8.3.1",
    "@next/eslint-plugin-next": "14.1.0",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-storybook": "0.6.15",
    "eslint-plugin-tailwindcss": "3.14.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "scaffdog": "3.0.0",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.2.1"
  },
  "optionalDependencies": {
    "@storybook/addon-actions": "7.6.10",
    "@storybook/addon-essentials": "7.6.10",
    "@storybook/addon-interactions": "7.6.10",
    "@storybook/addon-links": "7.6.10",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/nextjs": "7.6.10",
    "@storybook/react": "7.6.10",
    "@storybook/testing-library": "0.2.2",
    "husky": "9.1.6",
    "hygen": "6.2.11",
    "lint-staged": "15.2.0",
    "prettier": "3.2.4",
    "storybook": "7.6.10"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,ts,tsx,css,scss,json,html,graphql,md}": [
      "prettier --write --ignore-path .gitignore"
    ],
    "schema.prisma": [
      "prisma format"
    ]
  }
}
