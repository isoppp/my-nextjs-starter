  "author": "isoppp <prog@d2deck.com>",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext js,ts,tsx .",
    "generate:icon": "npx @svgr/cli --ext tsx --memo -d src/components/icon src/components/icon"
  },
  "husky": {
