{
  "name": "my-nextjs-starter",
  "version": "1.0.0",
  "main": "index.js",
  "packageManager": "pnpm@8.15.9",
  "scripts": {
    "start-by-docker": "echo $COMMIT_HASH && npx prisma migrate deploy && node server.js",
    "start": "npx prisma migrate deploy && next start",
    "dev": "PORT=3033 next dev",
    "build": "next build",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext ts,tsx . --ignore-path .gitignore",
    "format": "prettier --write './**/*.{js,ts,tsx,css,scss,json,html,graphql,md}' --ignore-path .gitignore",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "gen-c": "scaffdog generate react-component",
    "migrate": "dotenv -e .env.development -- prisma migrate dev",
    "migrate:reset": "dotenv -e .env.development -- prisma migrate reset",
    "migrate:reset:test": "dotenv -e .env.test -- prisma migrate reset --force --skip-seed",
    "migrate:deploy": "dotenv -e .env.production prisma migrate deploy",
    "prisma:studio": "dotenv -e .env.development -- prisma studio",
    "test": "dotenv -e .env -e .env.test -- prisma migrate reset --force --skip-seed && dotenv -e .env -e .env.test -- vitest",
    "test-ci": "dotenv -e .env.test -- prisma migrate reset --force --skip-seed && dotenv -e .env.test -- vitest --run --silent",
    "postinstall": "prisma generate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "3.9.0",
    "@prisma/client": "5.21.1",
    "@t3-oss/env-nextjs": "0.11.1",
    "@trpc/client": "10.45.2",
    "@trpc/next": "10.45.2",
    "@trpc/react-query": "10.45.2",
    "@trpc/server": "10.45.2",
    "@types/node": "20.17.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "autoprefixer": "10.4.20",
    "clsx": "2.1.1",
    "css-loader": "6.11.0",
    "dayjs": "1.11.13",
    "dotenv-cli": "7.4.2",
    "jotai": "2.10.1",
    "nanoid": "5.0.7",
    "next": "14.2.16",
    "pino": "8.21.0",
    "postcss": "8.4.47",
    "prisma": "5.21.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.53.1",
    "react-icons": "5.3.0",
    "remeda": "1.61.0",
    "sass": "1.80.4",
    "superjson": "2.2.1",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.3",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@next/eslint-plugin-next": "14.2.16",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-storybook": "0.10.1",
    "eslint-plugin-tailwindcss": "3.17.5",
    "eslint-plugin-unused-imports": "3.2.0",
    "scaffdog": "3.0.0",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.0"
  },
  "optionalDependencies": {
    "@storybook/addon-actions": "7.6.20",
    "@storybook/addon-essentials": "7.6.20",
    "@storybook/addon-interactions": "7.6.20",
    "@storybook/addon-links": "7.6.20",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/nextjs": "7.6.20",
    "@storybook/react": "7.6.20",
    "@storybook/testing-library": "0.2.2",
    "husky": "8.0.3",
    "hygen": "6.2.11",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "storybook": "7.6.20"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,ts,tsx,css,scss,json,html,graphql,md}": [
      "prettier --write --ignore-path .gitignore"
    ],
    "schema.prisma": [
      "prisma format"
    ]
  }
}
