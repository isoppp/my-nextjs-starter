    "react-hook-form": "^7.26.1",
    "react-icons": "^4.3.1",
    "typescript": "^4.5.5",
    "valtio": "^1.3.1",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@types/node": "17.0.38",
    "@types/react": "18.0.10",
    "@storybook/addon-actions": "6.5.6",
    "@storybook/addon-essentials": "6.5.6",
    "@storybook/addon-interactions": "6.5.6",
