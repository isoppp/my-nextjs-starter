    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tailwindcss": "3.5.0",
    "eslint-plugin-valtio": "0.4.4",
    "husky": "8.0.1",
    "hygen": "6.2.3",
    "lint-staged": "13.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
