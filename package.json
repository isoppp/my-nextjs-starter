  "license": "MIT",
  "scripts": {
    "dev": "PORT=3033 next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext js,ts,tsx . --ignore-path .gitignore",
    "format": "prettier --write './**/*.{js,ts,tsx,css,scss,json,html,graphql,md}' --ignore-path .gitignore",
    "storybook": "run-p storybook:*",
    "storybook:t": "tailwindcss -o .storybook/tailwind.css --watch",
    "storybook:s": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "gen:c": "hygen component new"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,ts,tsx,css,scss,json,html,graphql,md}": [
      "prettier --write --ignore-path .gitignore"
    ]
  },
  "dependencies": {
    "clsx": "^1.1.1",
