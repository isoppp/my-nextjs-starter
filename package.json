{
  "name": "my-nextjs-starter",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "pnpm": ">=8.6"
  },
  "scripts": {
    "start-by-docker": "echo $COMMIT_HASH && npx prisma migrate deploy && node server.js",
    "start": "npx prisma migrate deploy && next start",
    "dev": "PORT=3033 next dev",
    "build": "next build",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext ts,tsx . --ignore-path .gitignore",
    "format": "prettier --write './**/*.{js,ts,tsx,css,scss,json,html,graphql,md}' --ignore-path .gitignore",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "gen-c": "hygen component new",
    "migrate": "dotenv -e .env.development -- prisma migrate dev",
    "migrate:reset": "dotenv -e .env.development -- prisma migrate reset",
    "migrate:reset:test": "dotenv -e .env.test -- prisma migrate reset --force --skip-seed",
    "migrate:deploy": "dotenv -e .env.production prisma migrate deploy",
    "prisma:studio": "dotenv -e .env.development -- prisma studio",
    "test": "dotenv -e .env -e .env.test -- prisma migrate reset --force --skip-seed && dotenv -e .env -e .env.test -- vitest",
    "test-ci": "dotenv -e .env.test -- prisma migrate reset --force --skip-seed && dotenv -e .env.test -- vitest --run --silent",
    "postinstall": "prisma generate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.1",
    "@prisma/client": "5.4.1",
    "@svgr/webpack": "8.1.0",
    "@t3-oss/env-nextjs": "0.7.0",
    "@trpc/client": "10.40.0",
    "@trpc/next": "10.40.0",
    "@trpc/react-query": "10.40.0",
    "@trpc/server": "10.40.0",
    "@types/node": "18.18.4",
    "@types/react": "18.2.25",
    "@types/react-dom": "18.2.11",
    "autoprefixer": "10.4.16",
    "clsx": "2.0.0",
    "css-loader": "6.8.1",
    "dayjs": "1.11.10",
    "dotenv-cli": "7.3.0",
    "jotai": "2.4.3",
    "nanoid": "5.0.1",
    "next": "13.5.4",
    "pino": "8.15.6",
    "postcss": "8.4.31",
    "prisma": "5.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.47.0",
    "react-icons": "4.11.0",
    "remeda": "1.27.0",
    "sass": "1.69.0",
    "superjson": "2.2.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "8.1.0",
    "@next/eslint-plugin-next": "13.5.4",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-storybook": "0.6.15",
    "eslint-plugin-tailwindcss": "3.13.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  },
  "optionalDependencies": {
    "@storybook/addon-actions": "7.4.6",
    "@storybook/addon-essentials": "7.4.6",
    "@storybook/addon-interactions": "7.4.6",
    "@storybook/addon-links": "7.4.6",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/nextjs": "7.4.6",
    "@storybook/react": "7.4.6",
    "@storybook/testing-library": "0.2.2",
    "husky": "8.0.3",
    "hygen": "6.2.11",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "storybook": "7.4.6"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,ts,tsx,css,scss,json,html,graphql,md}": [
      "prettier --write --ignore-path .gitignore"
    ],
    "schema.prisma": [
      "prisma format"
    ]
  }
}
