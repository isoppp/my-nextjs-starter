  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext js,ts,tsx .",
    "format": "prettier --write './**/*.{js,ts,tsx,css,scss,json,html,graphql,md}' --ignore-path .gitignore",
    "prestorybook": "rimraf node_modules/.cache/storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install",
    "gen:c": "hygen component new"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,ts,tsx,css,scss,json,html,graphql,md}": [
      "prettier --write --ignore-path .gitignore"
    ]
  },
  "dependencies": {
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "next": "^12.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.25.0",
    "react-icons": "^4.3.1",
    "react-query": "^3.34.12",
    "recoil": "^0.5.2",
    "typescript": "^4.5.5",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
